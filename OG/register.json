{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://www.homeunion.com"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='getstart']//a[.='Get Started']"
      }
    },
    {
      "type": "pause",
      "waitTime": "2000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='pad_bgall']/div/div/div[2]/div[5]/button"
      }
    },
    {
      "type": "pause",
      "waitTime": "2000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "registersign_trig"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img.img-responsive"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "First Name"
    },
    {
      "type": "verifyTextPresent",
      "text": "Last Name"
    },
    {
      "type": "verifyTextPresent",
      "text": "Phone Number"
    },
    {
      "type": "verifyTextPresent",
      "text": "Email Address"
    },
    {
      "type": "verifyTextPresent",
      "text": "Create Password"
    },
    {
      "type": "verifyTextPresent",
      "text": "Confirm Password"
    },
    {
      "type": "verifyTextPresent",
      "text": "I agree to the Terms of Service and Privacy Policy"
    },
    {
      "type": "verifyTextPresent",
      "text": "I would like to receive investment alerts and investment news via email."
    },
    {
      "type": "verifyTextPresent",
      "text": "Already have an account? LOGIN"
    },
    {
      "type": "verifyTextPresent",
      "text": "Register"
    },
    {
      "type": "verifyTextPresent",
      "text": "Our algorithmic approach to market, neighborhood and property identification enable you to make smart buy/sell/hold decisions."
    },
    {
      "type": "verifyTextPresent",
      "text": "Invest Easier"
    },
    {
      "type": "verifyTextPresent",
      "text": "1-866-732-3220"
    },
    {
      "type": "verifyTextPresent",
      "text": "info@homeunion.com"
    },
    {
      "type": "verifyTextPresent",
      "text": "Live Chat"
    },
    {
      "type": "verifyTextPresent",
      "text": ""
    },
    {
      "type": "verifyTextPresent",
      "text": "Build Wealth"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}
