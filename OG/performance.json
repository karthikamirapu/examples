{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://investor.homeunion.com/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Get Started"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Login"
      }
    },
    {
      "type": "pause",
      "waitTime": ""
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-username-input"
      },
      "text": "demo@homeunion.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-password-input"
      },
      "text": "123456"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "login-btn"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Performance"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "INVESTMENT SUMMARY"
    },
    {
      "type": "verifyTextPresent",
      "text": "INCOME AND EXPENSES"
    },
    {
      "type": "verifyTextPresent",
      "text": "NET OPERATING INCOME"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='header-inner']/ul/li[2]/a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "logout-link"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}
