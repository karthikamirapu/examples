{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://investor.homeunion.com/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "open_login_modal"
      }
    },
    {
      "type": "pause",
      "waitTime": "500"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-username-input"
      },
      "text": "solutionspecialist@outlook.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "login-password-input"
      },
      "text": "123456"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "login-btn"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Property Search"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "Property Search"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//table[@id='tbl_search_results']/tbody/tr[1]/td[1]/div/a/img"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "Photos"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Street View"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div[1]/div[1]/div/section"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Financial Details"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "Investment Summary"
    },
    {
      "type": "verifyTextPresent",
      "text": "Annual Return Calculations"
    },
    {
      "type": "verifyTextPresent",
      "text": "Expenses"
    },
    {
      "type": "verifyTextPresent",
      "text": "Loan Terms"
    },
    {
      "type": "verifyTextPresent",
      "text": "FINANCIAL CALCULATOR"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Neighborhood"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "Distribution of Rents in Neighborhood"
    },
    {
      "type": "verifyTextPresent",
      "text": "My Rent:"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div[1]/div[3]/div/div/div[2]/div[1]/div[2]/svg"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Household Rent Affordability"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Local Area"
      }
    },
    {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='gm-style']/div[1]/div[2]"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Major Employers"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "City Info"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='header-inner']/ul/li[2]/a"
      }
    },
    {
      "type": "pause",
      "waitTime": "500"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "logout-link"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}
